<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ex.droidlist"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyViewFipperActivity"></activity>
        <activity
            android:name=".MaterialTabActivity"
            android:label="@string/title_activity_material_tab"></activity>
        <activity
            android:name=".CustomTabsActivity"
            android:label="@string/title_activity_custom_tabs"></activity>
        <activity
            android:name=".TestActivity"
            android:label="@string/title_activity_test"></activity>

        <service
            android:name="com.ex.droidlist.ExampleIntentService"
            android:exported="false"></service>
        <service android:name="com.ex.droidlist.MyService"></service>
        <service android:name="com.ex.droidlist.ForegroundService"></service>
    </application>

</manifest>